  },
  "license": "ISC",
  "types": "out/index.d.ts",
  "files": [
    "out",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^3.0.0-types.0",
    "@rbxts/types": "^1.0.813",
    "roblox-ts": "npm:@wad4444/roblox-ts@^1.0.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@rbxts/jabby": "^0.3.1",
    "@rbxts/planck": "^1.0.0"
  }
